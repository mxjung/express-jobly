{
  "$schema": "http://json-schema.org/draft-07/schema",
  "$id": "http://example.com/example.json",
  "type": "object",
  "title": "The Root Schema",
  "description": "The root schema comprises the entire JSON document.",
  "default": {},
  "additionalProperties": false,
  "anyOf": [
    {"required": ["handle"]}, {"required": ["name"]}â€©
  ],
  "properties": {
      "handle": {
          "$id": "#/properties/handle",
          "type": "string",
          "title": "The Handle Schema",
          "description": "short code that uniquely identifies company",
          "default": "",
          "examples": [
              "Fish"
          ]
      },
      "name": {
          "$id": "#/properties/name",
          "type": "string",
          "title": "The Name Schema",
          "description": "Name of company",
          "default": "",
          "examples": [
              "Fish tops"
          ]
      },
      "num_employees": {
          "$id": "#/properties/num_employees",
          "type": "integer",
          "title": "The Num_employees Schema",
          "description": "Number of employees",
          "default": 0,
          "examples": [
              80000.0
          ],
          "minimum": 1.0
      },
      "description": {
          "$id": "#/properties/description",
          "type": "string",
          "title": "The Description Schema",
          "description": "Description of company",
          "default": "",
          "examples": [
              "Fish shop"
          ]
      },
      "logo_url": {
          "$id": "#/properties/logo_url",
          "type": "string",
          "title": "The Logo_url Schema",
          "description": "URL for company logo",
          "default": "",
          "examples": [
              "http://fishworld.com/media/logo.jpg"
          ]
      }
  }
}