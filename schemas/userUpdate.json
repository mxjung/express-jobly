{
  "$schema": "http://json-schema.org/draft-07/schema",
  "$id": "http://example.com/example.json",
  "type": "object",
  "title": "The user Schema",
  "description": "The user schema for Jobly.",
  "default": {},
  "additionalProperties": true,
  "properties": {
      "username": {
          "$id": "#/properties/username",
          "type": "string",
          "title": "The Username Schema",
          "description": "User's username",
          "default": "",
          "examples": [
              "user1"
          ]
      },
      "password": {
          "$id": "#/properties/password",
          "type": "string",
          "title": "The Password Schema",
          "description": "User's password",
          "default": "",
          "examples": [
              "akdjfhbkajbnva"
          ]
      },
      "first_name": {
          "$id": "#/properties/first_name",
          "type": "string",
          "title": "The First_name Schema",
          "description": "User's first name",
          "default": "",
          "examples": [
              "Jamie"
          ]
      },
      "last_name": {
          "$id": "#/properties/last_name",
          "type": "string",
          "title": "The Last_name Schema",
          "description": "User's last name",
          "default": "",
          "examples": [
              "chow"
          ]
      },
      "email": {
          "$id": "#/properties/email",
          "type": "string",
          "title": "The Email Schema",
          "description": "Email for user. Must be unique",
          "default": "",
          "examples": [
              "email2@gmail.com"
          ]
      },
      "photo_url": {
          "$id": "#/properties/photo_url",
          "type": "string",
          "title": "The Photo_url Schema",
          "description": "URL for externally hosted photo",
          "default": "",
          "examples": [
              "http://www.somedomain.com/me.jpg"
          ]
      },
      "is_admin": {
          "$id": "#/properties/is_admin",
          "type": "boolean",
          "title": "The Is_admin Schema",
          "description": "Boolean, not required, defaults to false in database.",
          "default": false,
          "examples": [
              false
          ]
      }
  }
}